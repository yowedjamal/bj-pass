{
    "title": "bj-pass Authentication Widget",
    "description": "Documentation compl√®te du widget d'authentification bj-pass",
    "author": "yowedjamal",
    "language": "fr",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "highlight",
        "search",
        "lunr",
        "sharing",
        "fontsettings",
        "theme-default"
    ],
    "pluginsConfig": {
        "highlight": {
            "theme": "github"
        },
        "search": {
            "maxIndexSize": 100000,
            "ignoreCase": true
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "twitter", "google", "weibo", "instapaper"
            ]
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        }
    },
    "variables": {
        "version": "2.0.0",
        "github": "https://github.com/yowedjamal/bj-pass",
        "email": "yowedjamal@gmail.com"
    }
}