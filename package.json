{
  "name": "bj-pass-auth-widget",
  "version": "1.3.4",
  "description": "Widget d'authentification OAuth 2.0/OpenID Connect moderne et sécurisé",
  "main": "dist/bj-pass-auth-widget.cjs.js",
  "module": "dist/bj-pass-auth-widget.esm.js",
  "types": "dist/bj-pass-auth-widget.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bj-pass-auth-widget.esm.js",
      "require": "./dist/bj-pass-auth-widget.cjs.js",
      "types": "./dist/bj-pass-auth-widget.d.ts"
    },
    "./react": {
      "import": "./dist/bj-pass-auth-widget.esm.js",
      "require": "./dist/bj-pass-auth-widget.cjs.js",
      "types": "./dist/bj-pass-auth-widget.d.ts"
    },
    "./angular": {
      "import": "./dist/bj-pass-auth-widget.esm.js",
      "require": "./dist/bj-pass-auth-widget.cjs.js",
      "types": "./dist/bj-pass-auth-widget.d.ts"
    },
    "./umd": "./dist/bj-pass-auth-widget.umd.js"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:types && webpack --mode production",
    "build:dev": "npm run build:types && webpack --mode development",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "prepare": "npm run build"
  },
  "keywords": [
    "oauth",
    "openid-connect",
    "authentication",
    "widget",
    "auth",
    "security",
    "pkce",
    "bj-pass",
    "react",
    "nextjs",
    "angular",
    "typescript"
  ],
  "author": {
    "name": "yowedjamal",
    "email": "yowedjamal@gmail.com",
    "url": "https://github.com/yowedjamal"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yowedjamal/bj-pass.git"
  },
  "bugs": {
    "url": "https://github.com/yowedjamal/bj-pass/issues"
  },
  "homepage": "https://github.com/yowedjamal/bj-pass#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@angular/core": ">=12.0.0",
    "@angular/common": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^9.1.3",
    "core-js": "^3.36.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
