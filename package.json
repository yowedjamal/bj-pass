{
  "name": "bj-pass-auth-widget",
  "version": "1.2.1",
  "description": "Widget d'authentification OAuth 2.0/OpenID Connect moderne et sécurisé",
  "main": "dist/bj-pass-auth-widget.cjs.js",
  "module": "dist/bj-pass-auth-widget.esm.js",
  "browser": "dist/bj-pass-auth-widget.umd.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bj-pass-auth-widget.esm.js",
      "require": "./dist/bj-pass-auth-widget.cjs.js",
      "browser": "./dist/bj-pass-auth-widget.umd.js"
    },
    "./next": "./wrappers/next.js",
    "./vue": "./wrappers/vue.js",
    "./react": "./wrappers/react.js"
  },
  "files": [
    "dist/",
    "wrappers/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:webpack": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": "^3.0.0"
  },
  "keywords": [
    "oauth",
    "openid-connect",
    "authentication",
    "widget",
    "auth",
    "security",
    "pkce",
    "bj-pass"
  ],
  "author": {
    "name": "yowedjamal",
    "email": "yowedjamal@gmail.com",
    "url": "https://github.com/yowedjamal"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yowedjamal/bj-pass.git"
  },
  "bugs": {
    "url": "https://github.com/yowedjamal/bj-pass/issues"
  },
  "homepage": "https://github.com/yowedjamal/bj-pass#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
